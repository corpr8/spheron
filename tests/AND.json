{
	"config": {
		"bigstep": true
	},
	"io": {
		"input1": {"type": "input", "angle": 0, "val": 0},
		"input2": {"type": "input", "angle": 0, "val": 0},
		"ANDout": {"type": "output", "angle": -60, "val": -1}
	},
	"tests": {
		"test1": {
			"inputs": {
				"input1": {"type":"input", "val": 0},
				"input2": {"type":"input", "val": 0}
			},
			"expected" : {
				"ANDout" : 0
			}
		},
		"test2": {
			"inputs": {
				"input1": {"type":"input", "val": 0},
				"input2": {"type":"input", "val": 1}
			},
			"expected" : {
				"ANDout" : 0.5
			}
		},
		"test3": {
			"inputs": {
				"input1": {"type":"input", "val": 1},
				"input2": {"type":"input", "val": 0}
			},
			"expected" : {
				"ANDout" : 0.5
			}
		},
		"test4": {
			"inputs":{
				"input1": {"type":"input", "val": 1},
				"input2": {"type":"input", "val": 1}
			},
			"expected" : {
				"ANDout" : 1
			}
		}
	}
}